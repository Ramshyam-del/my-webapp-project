{
  "name": "quantex-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "npm install --legacy-peer-deps && next build",
    "start": "next start -p 3000",
    "start:prod": "node start-production.js",
    "lint": "next lint",
    "format": "prettier --write .",
    "clean": "rimraf .next node_modules package-lock.json",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run reinstall && next build",
    "clean:build": "rimraf .next && npm run build",
    "postinstall": "node -e \"try { require('fs').rmSync('.next', { recursive: true, force: true }); } catch (e) { console.log('No .next directory to clean'); }\"",
    "ci:build": "npm ci && next build",
    "audit:secrets:win": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"Select-String -Path * -Pattern 'eyJ|SUPABASE|API_KEY|Bearer |@Million|admin@quantex' -Recurse | Select Path,LineNumber\"",
    "audit:secrets:nix": "grep -RIn \"eyJ\\|SUPABASE\\|API_KEY\\|Bearer \\|@Million\\|admin@quantex\" --exclude-dir=node_modules --exclude=*.lock .",
    "test:supabase": "node scripts/test-supabase.js",
    "check-env": "node -e \"require('dotenv').config({path:'./backend/.env'}); console.log('URL=',!!process.env.SUPABASE_URL,' SRK=',(process.env.SUPABASE_SERVICE_ROLE_KEY||'').slice(0,6))\"",
    "start:backend": "node backend/server.js"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@rainbow-me/rainbowkit": "^2.1.0",
    "@supabase/supabase-js": "^2.53.0",
    "autoprefixer": "^10.4.20",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "framer-motion": "^11.14.4",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.539.0",
    "next": "15.5.7",
    "node-fetch": "^3.3.2",
    "papaparse": "^5.5.3",
    "postcss": "^8.4.49",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.6.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "viem": "^2.37.2",
    "wagmi": "^2.12.0"
  },
  "devDependencies": {
    "eslint": "^8.40.0",
    "eslint-config-next": "^13.5.11",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.5"
  }
}
